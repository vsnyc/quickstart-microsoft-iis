        {
          "sectionId":"activeDirectoryConfigurationSection",
          "sectionTitle":"Active Directory configuration",
          "sectionOrder":3,
          "fields":[
            {
              "fieldId":"DomainAdminUser",
              "fieldType":"TextInput",
              "title":"Domain Admin user name",
              "description": "User name for the domain administrator account. This is separate from the default administrator account. Note: This is for a self-managed Active Directory scenario. When using AWS Directory Service for Microsoft Active Directory, \"Admin\" is the default regardless of the value provided.      ",
              "hintText":"Must be at least 8 characters containing letters and numbers.",
              "validationRules":{
                "minLength":"5",
                "maxLength":"25",
                "validationType":"AlphaNumeric"
              },
              "defaultValue":"Admin",
              "isRequired":true
            },
            {
              "fieldId":"DomainAdminPassword",
              "fieldType":"Password",
              "title":"Admin password",
              "description": "Password for the domain administrator. Must be at least 8 characters containing letters, numbers, and symbols.",
              "placeholder":"Enter password",
              "hintText":"Must be at least 8 characters containing letters, numbers and symbols",
              "validationRules":{
                "minLength":"8",
                "maxLength":"32",
                "customRegex":"(?=^.{6,255}$)((?=.*\\d)(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[^A-Za-z0-9])(?=.*[a-z])|(?=.*[^A-Za-z0-9])(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[A-Z])(?=.*[^A-Za-z0-9]))^.*"
              },
              "defaultValue":"",
              "isRequired":true
            },
            {
              "fieldId":"DomainNetBIOSName",
              "fieldType":"TextInput",
              "title":"Domain NetBIOS name",
              "validationRules":{
                "minLength":"1",
                "maxLength":"15",
                "customRegex":"^[^.][a-zA-Z0-9.]+[^:*?<>|`\\\\`]$"
              },
              "defaultValue":"EXAMPLE",
              "description": "NetBIOS name of the domain for users of earlier Windows versions (up to 15 characters).",
              "isRequired":true
            },
            {
              "fieldId":"DomainDNSName",
              "fieldType":"TextInput",
              "title":"Domain DNS name",
              "description": "Fully qualified domain name (FQDN) of the forest root domain.",
              "validationRules":{
                "minLength":"2",
                "maxLength":"255",
                "customRegex":"[a-zA-Z0-9\\-]+\\..+"
              },
              "defaultValue":"example.com",
              "isRequired":true
            }
          ]
        },
        {
          "sectionId":"webpageConfigurations",
          "sectionTitle":"IIS Webpage configurations",
          "sectionOrder":6,
          "sectionHelpPanelHeader":"IIS Webpage configurations",
          "fields":[
            {
              "fieldId":"WebBucketName",
              "fieldType":"TextInput",
              "title":"S3 Bucket Webpage Location",
              "description": "Bucket name where html file is located for iis. If left blank, a sample page will be used.",
              "defaultValue": "webfiles/index.html",
              "isRequired":true
            },
            {
              "fieldId":"WebBucketKey",
              "fieldType":"TextInput",
              "title":"S3 Key Webpage Location",
              "description": "Bucket Key where html file is located for iis. Only change S3 Bucket Wepage is specified, otherwise leave as default.",
              "defaultValue": ""
            }
          ]
        },
        {
          "sectionId":"autoScalingGroupConfigurations",
          "sectionTitle":"Auto Scaling group / Application Load Balancer configuration",
          "sectionOrder":7,
          "sectionHelpPanelHeader":"Auto Scaling group / Application Load Balancer configuration",
          "fields":[
            {
              "fieldId":"ASGDesiredCapacity",
              "fieldType":"NumericInput",
              "title":"Desired Capacity",
              "placeholder":"Enter number",
              "description": "Desired capacity of the Auto Scaling group.",
              "hintText":"A minimum number of 2 and maximum number of 4 servers is allowed.",
              "isRequired":true,
              "validationRules":{
                "validationType":"Numeric",
                "min":2,
                "max":4
              }
            },
            {
              "fieldId":"ASGMaxSize",
              "fieldType":"NumericInput",
              "title":"Max Capacity",
              "placeholder":"Enter number",
              "description": "Maximum instance size for the Auto Scaling group.",
              "hintText":"A minimum number of 2 and maximum number of 4 servers is allowed.",
              "isRequired":true,
              "validationRules":{
                "validationType":"Numeric",
                "min":2,
                "max":4
              }
            },
            {
              "fieldId":"ASGMinSize",
              "fieldType":"NumericInput",
              "title":"Min Capacity",
              "placeholder":"Enter number",
              "description": "Minimum instance size for the Auto Scaling group.",
              "hintText":"A minimum number of 2 and maximum number of 4 servers is allowed.",
              "isRequired":true,
              "validationRules":{
                "validationType":"Numeric",
                "min":2,
                "max":4
              }
            },
            {
              "fieldId":"WebAccessCIDR",
              "fieldType":"TextInput",
              "title":"WebAccess CIDR ",
              "description": "The CIDR IP range that is permitted to access the Elastic Load Balancers.",
              "validationRules":{
                "validationType":"isValidCidr"
              },
              "defaultValue":"10.0.0.0/16",
              "isRequired":true
            },
            {
              "fieldId":"ELBSchemeParameter",
              "fieldType":"StaticDropdown",
              "title":"Deployment Type",
              "description": "Choose whether the Elastic Load Balancer is public or private.",
              "placeholder":"Placeholder text",
              "defaultValue":"internet-facing",
              "isRequired":true,
              "isProvisioningParameter":true,
              "dropdownValues":[
                {
                  "label":"internet-facing",
                  "value":"internet-facing"
                },
                {
                  "label":"internal",
                  "value":"internal"
                }
              ]
            }
          ]
        }
