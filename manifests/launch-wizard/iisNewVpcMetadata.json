              "validationRules":{
                "validationType":"isValidCidr"
              },
              "defaultValue":"10.0.32.0/19",
              "isRequired":true
            },
            {
              "fieldId":"PrivateSubnet3CIDR",
              "fieldType":"TextInput",
              "title":"Private subnet 3 CIDR",
              "description":"CIDR block for private subnet 3 located in Availability Zone 3.",
              "validationRules":{
                "validationType":"isValidCidr"
              },
              "defaultValue":"10.0.64.0/19",
