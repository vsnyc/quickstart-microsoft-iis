                          "placeholder":"Placeholder text",
                          "defaultValue":"4",
                          "isRequired":true,
                          "isProvisioningParameter":false,
                          "dropdownValues":[
                            {
                              "id":"0",
                              "label":"4 Gb",
                              "value":"4"
                            },
                            {
                              "id":"1",
                              "label":"8 Gb",
                              "value":"8"
                            }
                          ]
                        },
                        {
                          "fieldId":"IISServerInstanceType",
