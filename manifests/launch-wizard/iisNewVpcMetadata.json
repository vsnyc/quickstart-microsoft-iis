            {
              "fieldId":"lwdkGeneralCardWidget",
              "fieldType":"LWDKGeneralComponent",
              "isRequired":true,
              "generalCardWidget":{
                "fieldComponents":[
                  "applicationName",
                  "snsTopic",
                  "disableRollback",
                  "applicationTags"
                ],
                "titles":[
                  "Deployment name",
                  "Simple Notification Service (SNS) topic ARN",
                  "Deactivate rollback on failed deployment",
                  "Tags "
                ],
                "descriptions":[
                  "Enter a unique name for this deployment.",
                  "Enter an SNS topic for AWS Launch Wizard to send notifications and alerts.",
                  "When a deployment fails, your provisioned resources will be deleted/rolled back by default. If deactivated, the provisioned resources will be deleted when you delete your deployment from the Launch Wizard console."
                ],
                "hintTexts":[
                  "10 characters maximum. Only alphanumeric characters are allowed.",
                  {
                    "text":"Create new SNS topic",
                    "link":"https://console.aws.amazon.com/sns/v3/home#/create-topic"
                  },
                  "When you select this option, your provisioned resources will not be deleted/rolled back after the deployment fails.",
                  null
                ],
                "errorTexts":[
                  "Enter a unique application name",
                  "SNS topic not found",
                  "Required field",
                  "Required field"
                ],
                "validationRules":[
                  {
                    "key":"applicationName",
                    "minLength":"1",
                    "maxLength":"10",
                    "validationType":"AlphaNumericStartWithAlpha"
                  },
                  {
                    "key":"AWS:LaunchWizard:TopicArn"
                  },
                  {
                    "key":"AWS:LaunchWizard:DisableRollbackFlag",
                    "validationType":"isBoolean"
                  },
                  {
                    "key":"AWS:LaunchWizard:CustomerTags",
                    "maxLength":"10",
                    "validationType":"AlphaNumeric"
                  }
                ],
                "provisioningParameters":[
                  "applicationName",
                  "AWS:LaunchWizard:TopicArn",
                  "AWS:LaunchWizard:DisableRollbackFlag",
                  "AWS:LaunchWizard:CustomerTags"
                ],
                "isRequired":[
                  true,
                  false,
                  false,
                  false
                ]
              }
            }
          ]
