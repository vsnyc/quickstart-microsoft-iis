        {
          "sectionId":"activeDirectoryConfigurationSection",
          "sectionTitle":"Active Directory configuration",
          "sectionOrder":3,
          "fields":[
            {
              "fieldId":"ADScenarioType",
              "fieldType":"StaticDropdown",
              "title":"Active Directory scenario type",
              "description":"Select the type of AD to use, either AWS Directory Service for Microsoft Active Directory or managing your own Amazon EC2 Active Directory instances.",
