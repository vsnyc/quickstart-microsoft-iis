              "isRequired":true
            },
            {
              "fieldId":"ADServer1PrivateIP",
              "fieldType":"TextInput",
              "title":"Domain Controller 1 private IP address",
              "description":"Fixed private IP address for the first Active Directory server, located in Availability Zone 1.",
              "validationRules":{
                "customRegex":"^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"
