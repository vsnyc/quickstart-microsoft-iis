              "isRequired":true,
              "isProvisioningParameter":true,
              "dropdownValues":[
                {
                  "label":"AWS Directory Service for Microsoft AD",
                  "value":"AWS Directory Service for Microsoft AD"
                },
                {
                  "label":"Microsoft AD on Amazon EC2",
                  "value":"Microsoft AD on Amazon EC2"
                }
              ]
            },
            {
              "fieldId":"DomainAdminUser",
              "fieldType":"TextInput",
              "title":"Domain Admin user name",
              "description":"User name for the new local administrator account.",
              "hintText":"Must be at least 8 characters containing letters and numbers.",
              "validationRules":{
                "minLength":"5",
                "maxLength":"25",
                "validationType":"AlphaNumeric"
              },
              "defaultValue":"Admin",
              "isRequired":true
            },
            {
              "fieldId":"DomainAdminPassword",
              "fieldType":"Password",
              "title":"Admin password",
              "description":"Password for the administrative account.",
              "placeholder":"Enter password",
              "hintText":"Must be at least 8 characters containing letters, numbers and symbols",
              "validationRules":{
                "minLength":"8",
                "maxLength":"32",
                "customRegex":"(?=^.{6,255}$)((?=.*\\d)(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[^A-Za-z0-9])(?=.*[a-z])|(?=.*[^A-Za-z0-9])(?=.*[A-Z])(?=.*[a-z])|(?=.*\\d)(?=.*[A-Z])(?=.*[^A-Za-z0-9]))^.*"
              },
              "defaultValue":"",
              "isRequired":true
            },
            {
              "fieldId":"DomainNetBIOSName",
              "fieldType":"TextInput",
              "title":"Domain NetBIOS name",
              "validationRules":{
                "minLength":"1",
                "maxLength":"15",
                "customRegex":"^[^.][a-zA-Z0-9.]+[^:*?<>|`\\\\`]$"
              },
              "defaultValue":"EXAMPLE",
              "description": "NetBIOS name of the domain for users of earlier Windows versions (up to 15 characters).",
              "isRequired":true
            },
            {
              "fieldId":"DomainDNSName",
              "fieldType":"TextInput",
              "title":"Domain DNS name",
              "description": "Fully qualified domain name (FQDN) of the forest root domain.",
              "validationRules":{
                "minLength":"2",
                "maxLength":"255",
                "customRegex":"[a-zA-Z0-9\\-]+\\..+"
              },
              "defaultValue":"example.com",
              "isRequired":true
            },
            {
              "fieldId":"ADServer1NetBIOSName",
              "fieldType":"TextInput",
              "title":"Domain Controller 1 NetBIOS Name",
              "description":"NetBIOS name of the first Active Directory server (up to 15 characters).",
